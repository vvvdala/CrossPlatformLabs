@model Newtonsoft.Json.Linq.JObject

<h1>Booking Details</h1>

<table>
    <tr>
        <th>Booking ID</th>
        <td>@Model["bookingId"]</td>
    </tr>
    <tr>
        <th>Customer Name</th>
        <td>@Model["customerName"]</td>
    </tr>
    <tr>
        <th>Vehicle Registration Number</th>
        <td>@Model["vehicleRegNumber"]</td>
    </tr>
    <tr>
        <th>Status Code</th>
        <td>@Model["statusCode"]</td>
    </tr>
    <tr>
        <th>Status Description</th>
        <td>@Model["statusDescr"]</td>
    </tr>
</table>